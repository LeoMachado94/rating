{
    "name": "LeoMachado94/rating",
    "description": "Laravel Eloquent Rating allows you to assign ratings to any model.",
    "keywords": [
        "eloquent",
        "laravel",
        "model",
        "rating",
        "rate",
        "stars",
        "star",
        "system"
    ],
    "homepage": "https://github.com/LeoMachado94/rating",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Leonardo Machado Mengue",
            "email": "leo94.machado@gmail.com",
            "homepage": "https://github.com/LeoMachado94",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/database": "^10.1",
        "illuminate/support": "^10.1"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.3",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^6.28|^7.0",
        "orchestra/testbench-core": "^6.28|^7.0",
        "phpunit/phpunit": "^9.5.21"
    },
    "autoload": {
        "psr-4": {
            "Rennokki\\Rating\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rennokki\\Rating\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rennokki\\Rating\\RatingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
